import java.util.Scanner;
public class fibo{
    public static void main(String args[]){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        for (int i = 0;i<=n;i++) {
            System.out.print(fibonacci(i) + " ");
            
        }
    }
    static int fibonacci(int n){
        if(n == 0 || n == 1){
            return n;
        }
        else{
            return fibonacci(n-1) + fibonacci(n-2);
        }
    }
}
