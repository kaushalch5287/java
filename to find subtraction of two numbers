import java.util.Scanner;

class main{
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("enter first number: ");
        int a = input.nextInt();
        System.out.print("enter second number:");
        int b = input.nextInt();
        int subtract = a - b;
        System.out.println( "result: " + subtract);


    }
}
